{
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "plugins": ["react"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": "pages/api",
      "extends": "eslint:recommended",
      "rules": { "no-console": "warn" }
    }
  ],
  "env": { "browser": true, "node": true, "shared-node-browser": true },
  "rules": {}
}
